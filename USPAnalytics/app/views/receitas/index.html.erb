<p id="notice"><%= notice %></p>

<h1>Receitas</h1>

<table>
  <thead>
    <tr>
      <th>Receita tesouro estado</th>
      <th>Receita propria nao vinculada</th>
      <th>Rendimento aplicacoes</th>
      <th>Reembolsos</th>
      <th>Outras receitas</th>
      <th>Receitas vinculadas</th>
      <th>Ano</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receitas.each do |receita| %>
      <tr>
        <td><%= receita.receita_tesouro_estado %></td>
        <td><%= receita.receita_propria_nao_vinculada %></td>
        <td><%= receita.rendimento_aplicacoes %></td>
        <td><%= receita.reembolsos %></td>
        <td><%= receita.outras_receitas %></td>
        <td><%= receita.receitas_vinculadas %></td>
        <td><%= receita.ano %></td>
        <td><%= link_to 'Show', receita %></td>
        <td><%= link_to 'Edit', edit_receita_path(receita) %></td>
        <td><%= link_to 'Destroy', receita, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receita', new_receita_path %>
